<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.nba.dao.TeamDao">
    <select id="getAllTeam" resultType="com.cn.nba.entity.Team">
        select * from team
    </select>


    <update id="updateTeam">
        update team set `name` = "aa" where id=1
    </update>

    <update id="updateTeam1">
        update team set `name` = "aa" where id=2
    </update>

    <insert id="saveTeam">
        insert team (name)values('123')
    </insert>
</mapper>